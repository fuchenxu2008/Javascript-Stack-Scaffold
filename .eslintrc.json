{
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "plugins": [
        "compat",
        "flowtype"
    ],
    "rules": {
        "indent": [
            "error", 4
        ],
        "no-unexpected-multiline": 2,
        "compat/compat": 2
    },
    "parser": "babel-eslint",
    "env": {
        "jest": true
    }
}